[openSessionTiming]
waitForNotepadReady=500
waitForSessionManagerLoop=1000
waitForEditReady=500
waitDokListOpenLoop=500

[gui]
dpiScale=96
windowPosX=307
windowPosY=50
clientWidth=1276
clientHeight=681
currentSelected=07
